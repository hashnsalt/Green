\n\r\n    &.box02 {margin: 0 10px};\r\n\r\n    transition: all 0.3s;\r\n\r\n    @for $i from 1 through 3 {\r\n      //변수 i는 1부터 3까지 돌아간다.\r\n\r\n      &:nth-child(#{$i}) {\r\n        //nth-child괄호 안의 값은 문자형이 들어가야 한다. 그러므로 자바스크립트에서 사용하던 백틱 기호 대신 '#'을 이용한다.\r\n        background-image: url(./images/new0#{$i}_off.jpg);\r\n        background-size: cover;\r\n      }\r\n      &:nth-child(#{$i}):hover{\r\n        background-image: url(./images/new0#{$i}_on.jpg);\r\n        background-size: cover;\r\n      }\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"2094ff2d276add361c994d8037f1b191","cacheData":{"env":{}}}